#!/usr/bin/env bash
SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
MAC10_BIN="/mnt/c/Users/Owner/Desktop/setup-agents-mac8/setup-agents-mac10/coordinator/bin/mac10"
if [ ! -f "$MAC10_BIN" ]; then
  echo "ERROR: mac10 CLI not found at $MAC10_BIN" >&2
  echo "  Has the setup-agents repo moved? Re-run setup.sh to fix." >&2
  exit 1
fi
exec node "$MAC10_BIN" "$@"
